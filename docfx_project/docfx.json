{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "**.csproj"
        ],
        "src": "../lib/PuppeteerSharp"
        }
      ],
      "dest": "api",
      "disableGitFeatures": false,
      "properties": {
        "TargetFramework": "net8"
    },
    "allowCompilationErrors": true
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "examples/**.yml",
          "examples/**.md"
        ]
      },
      {
        "files": [
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": ".",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
    "default",
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false,
    "sitemap":{
        "baseUrl": "https://www.puppeteersharp.com/",
        "priority": 1
    }
  }
}
